{
  "extends": "../../tsconfig.json",
  "include": ["src", "__tests__/**/*.ts"],
  "references": [
    { "path": "../api-file-manager" },
    { "path": "../api-page-builder" },
    { "path": "../api-security" },
    { "path": "../error" },
    { "path": "../handler" },
    { "path": "../handler-args" },
    { "path": "../handler-aws" },
    { "path": "../handler-graphql" },
    { "path": "../utils" },
    { "path": "../validation" },
    { "path": "../api-dynamodb-to-elasticsearch" },
    { "path": "../api-file-manager-ddb-es" },
    { "path": "../api-i18n-ddb" },
    { "path": "../api-security-so-ddb" },
    { "path": "../api-tenancy" },
    { "path": "../api-tenancy-so-ddb" },
    { "path": "../api-wcp" },
    { "path": "../db" }
  ],
  "compilerOptions": {
    "rootDirs": ["./src", "./__tests__"],
    "outDir": "./dist",
    "declarationDir": "./dist",
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/api-file-manager/*": ["../api-file-manager/src/*"],
      "@webiny/api-file-manager": ["../api-file-manager/src"],
      "@webiny/api-page-builder/*": ["../api-page-builder/src/*"],
      "@webiny/api-page-builder": ["../api-page-builder/src"],
      "@webiny/api-security/*": ["../api-security/src/*"],
      "@webiny/api-security": ["../api-security/src"],
      "@webiny/error/*": ["../error/src/*"],
      "@webiny/error": ["../error/src"],
      "@webiny/handler/*": ["../handler/src/*"],
      "@webiny/handler": ["../handler/src"],
      "@webiny/handler-args/*": ["../handler-args/src/*"],
      "@webiny/handler-args": ["../handler-args/src"],
      "@webiny/handler-aws/*": ["../handler-aws/src/*"],
      "@webiny/handler-aws": ["../handler-aws/src"],
      "@webiny/handler-graphql/*": ["../handler-graphql/src/*"],
      "@webiny/handler-graphql": ["../handler-graphql/src"],
      "@webiny/utils/*": ["../utils/src/*"],
      "@webiny/utils": ["../utils/src"],
      "@webiny/validation/*": ["../validation/src/*"],
      "@webiny/validation": ["../validation/src"],
      "@webiny/api-dynamodb-to-elasticsearch/*": ["../api-dynamodb-to-elasticsearch/src/*"],
      "@webiny/api-dynamodb-to-elasticsearch": ["../api-dynamodb-to-elasticsearch/src"],
      "@webiny/api-file-manager-ddb-es/*": ["../api-file-manager-ddb-es/src/*"],
      "@webiny/api-file-manager-ddb-es": ["../api-file-manager-ddb-es/src"],
      "@webiny/api-i18n-ddb/*": ["../api-i18n-ddb/src/*"],
      "@webiny/api-i18n-ddb": ["../api-i18n-ddb/src"],
      "@webiny/api-security-so-ddb/*": ["../api-security-so-ddb/src/*"],
      "@webiny/api-security-so-ddb": ["../api-security-so-ddb/src"],
      "@webiny/api-tenancy/*": ["../api-tenancy/src/*"],
      "@webiny/api-tenancy": ["../api-tenancy/src"],
      "@webiny/api-tenancy-so-ddb/*": ["../api-tenancy-so-ddb/src/*"],
      "@webiny/api-tenancy-so-ddb": ["../api-tenancy-so-ddb/src"],
      "@webiny/api-wcp/*": ["../api-wcp/src/*"],
      "@webiny/api-wcp": ["../api-wcp/src"],
      "@webiny/db/*": ["../db/src/*"],
      "@webiny/db": ["../db/src"]
    },
    "baseUrl": "."
  }
}

{
  "name": "@webiny/cli",
  "private": true,
  "version": "0.2.0",
  "main": "index.js",
  "bin": {
    "webiny": "./index.js",
    "wby": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webiny/webiny-js.git"
  },
  "author": "Pavel Denisjuk <pavel@webiny.com>",
  "files": [
    "lib",
    "lib/**/.gitignore"
  ],
  "description": "A tool to bootstrap a Webiny project.",
  "dependencies": {
    "@webiny/tracking": "^0.1.2",
    "chalk": "^2.4.2",
    "crypto": "^1.0.1",
    "execa": "^2.0.5",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "load-json-file": "^6.2.0",
    "uuid": "^3.3.3",
    "write-json-file": "^4.2.0",
    "yargs": "^14.0.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf ./lib && babel src -d dist --source-maps --copy-files --ignore=src/init/template --include-dotfiles",
    "build:watch": "rimraf ./lib && babel src -d lib --source-maps --copy-files --ignore=src/init/template --watch -v --include-dotfiles",
    "postbuild": "cp package.json LICENSE README.md dist/",
    "prepublishOnly": "yarn build"
  }
}

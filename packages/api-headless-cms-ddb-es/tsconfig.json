{
  "extends": "../../tsconfig.json",
  "include": ["src", "__tests__/**/*.ts"],
  "references": [
    { "path": "../api" },
    { "path": "../api-elasticsearch" },
    { "path": "../api-headless-cms" },
    { "path": "../db-dynamodb" },
    { "path": "../error" },
    { "path": "../api-i18n" },
    { "path": "../api-i18n-ddb" },
    { "path": "../api-security" },
    { "path": "../api-security-so-ddb" },
    { "path": "../api-tenancy" },
    { "path": "../api-tenancy-so-ddb" },
    { "path": "../handler" },
    { "path": "../handler-db" },
    { "path": "../plugins" },
    { "path": "../utils" },
    { "path": "../api-dynamodb-to-elasticsearch" },
    { "path": "../handler-aws" }
  ],
  "compilerOptions": {
    "rootDirs": ["./src", "./__tests__"],
    "outDir": "./dist",
    "declarationDir": "./dist",
    "paths": {
      "~/*": ["./src/*"],
      "~tests/*": ["./__tests__/*"],
      "@webiny/api/*": ["../api/src/*"],
      "@webiny/api": ["../api/src"],
      "@webiny/api-elasticsearch/*": ["../api-elasticsearch/src/*"],
      "@webiny/api-elasticsearch": ["../api-elasticsearch/src"],
      "@webiny/api-headless-cms/*": ["../api-headless-cms/src/*"],
      "@webiny/api-headless-cms": ["../api-headless-cms/src"],
      "@webiny/db-dynamodb/*": ["../db-dynamodb/src/*"],
      "@webiny/db-dynamodb": ["../db-dynamodb/src"],
      "@webiny/error/*": ["../error/src/*"],
      "@webiny/error": ["../error/src"],
      "@webiny/api-i18n/*": ["../api-i18n/src/*"],
      "@webiny/api-i18n": ["../api-i18n/src"],
      "@webiny/api-i18n-ddb/*": ["../api-i18n-ddb/src/*"],
      "@webiny/api-i18n-ddb": ["../api-i18n-ddb/src"],
      "@webiny/api-security/*": ["../api-security/src/*"],
      "@webiny/api-security": ["../api-security/src"],
      "@webiny/api-security-so-ddb/*": ["../api-security-so-ddb/src/*"],
      "@webiny/api-security-so-ddb": ["../api-security-so-ddb/src"],
      "@webiny/api-tenancy/*": ["../api-tenancy/src/*"],
      "@webiny/api-tenancy": ["../api-tenancy/src"],
      "@webiny/api-tenancy-so-ddb/*": ["../api-tenancy-so-ddb/src/*"],
      "@webiny/api-tenancy-so-ddb": ["../api-tenancy-so-ddb/src"],
      "@webiny/handler/*": ["../handler/src/*"],
      "@webiny/handler": ["../handler/src"],
      "@webiny/handler-db/*": ["../handler-db/src/*"],
      "@webiny/handler-db": ["../handler-db/src"],
      "@webiny/plugins/*": ["../plugins/src/*"],
      "@webiny/plugins": ["../plugins/src"],
      "@webiny/utils/*": ["../utils/src/*"],
      "@webiny/utils": ["../utils/src"],
      "@webiny/api-dynamodb-to-elasticsearch/*": ["../api-dynamodb-to-elasticsearch/src/*"],
      "@webiny/api-dynamodb-to-elasticsearch": ["../api-dynamodb-to-elasticsearch/src"],
      "@webiny/handler-aws/*": ["../handler-aws/src/*"],
      "@webiny/handler-aws": ["../handler-aws/src"]
    },
    "baseUrl": "."
  }
}

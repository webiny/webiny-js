	"repository": {
		"type": "git",
		"url": "https://github.com/webiny/webiny-js.git"
	},
	"author": "Webiny Ltd",
	"license": "MIT",
	"dependencies": {
		"@webiny/app": "^5.0.0-beta.0",
		"@webiny/app-admin": "^5.0.0-beta.0",
		"@webiny/app-i18n": "^5.0.0-beta.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "^7.5.5",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.8.3",
		"@emotion/styled": "^10.0.27",
		"@webiny/app-security": "^5.0.0-beta.0",
		"@webiny/plugins": "^5.0.0-beta.0",
		"@webiny/react-router": "^5.0.0-beta.0",
		"@webiny/ui": "^5.0.0-beta.0",
		"apollo-client": "^2.6.10",
		"babel-plugin-emotion": "^9.2.8",
		"babel-plugin-lodash": "^3.3.4",
		"emotion": "^10.0.17",
		"react-helmet": "^5.2.0"
	},
	"peerDependencies": {
		"react": "16.14.0",
		"react-dom": "16.14.0"
	},
	"publishConfig": {
		"access": "public",
		"directory": "dist"
	},
	"scripts": {
		"build": "rimraf ./dist *.tsbuildinfo && babel src -d dist --source-maps --copy-files --extensions \".ts,.tsx\"",
		"watch": "babel src -d dist --source-maps --copy-files --extensions \".ts,.tsx\" --watch",
		"postbuild": "cp package.json LICENSE README.md dist/ && tsc -p tsconfig.build.json"
	},
	"svgo": {
		"plugins": {
			"removeViewBox": false
		}
	},
	"adio": {
		"ignore": {
			"peerDependencies": [
				"react-dom"
			]
		}
	}
}

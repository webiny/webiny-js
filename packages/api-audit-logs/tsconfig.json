{
  "extends": "../../tsconfig.json",
  "include": ["src", "__tests__"],
  "references": [
    { "path": "../api" },
    { "path": "../api-aco" },
    { "path": "../api-admin-users-cognito" },
    { "path": "../api-apw" },
    { "path": "../api-form-builder" },
    { "path": "../api-mailer" },
    { "path": "../api-page-builder" },
    { "path": "../api-page-builder-import-export" },
    { "path": "../error" },
    { "path": "../handler" },
    { "path": "../api-headless-cms" },
    { "path": "../api-i18n" },
    { "path": "../api-security" },
    { "path": "../api-tenancy" },
    { "path": "../handler-aws" },
    { "path": "../handler-graphql" },
    { "path": "../plugins" }
  ],
  "compilerOptions": {
    "rootDirs": ["./src", "./__tests__"],
    "outDir": "./dist",
    "declarationDir": "./dist",
    "paths": {
      "~/*": ["./src/*"],
      "~tests/*": ["./__tests__/*"],
      "@webiny/api/*": ["../api/src/*"],
      "@webiny/api": ["../api/src"],
      "@webiny/api-aco/*": ["../api-aco/src/*"],
      "@webiny/api-aco": ["../api-aco/src"],
      "@webiny/api-admin-users-cognito/*": ["../api-admin-users-cognito/src/*"],
      "@webiny/api-admin-users-cognito": ["../api-admin-users-cognito/src"],
      "@webiny/api-apw/*": ["../api-apw/src/*"],
      "@webiny/api-apw": ["../api-apw/src"],
      "@webiny/api-form-builder/*": ["../api-form-builder/src/*"],
      "@webiny/api-form-builder": ["../api-form-builder/src"],
      "@webiny/api-mailer/*": ["../api-form-mailer/src/*"],
      "@webiny/api-mailer": ["../api-form-mailer/src"],
      "@webiny/api-page-builder/*": ["../api-page-builder/src/*"],
      "@webiny/api-page-builder": ["../api-page-builder/src"],
      "@webiny/api-page-builder-import-export/*": ["../api-page-builder-import-export/src/*"],
      "@webiny/api-page-builder-import-export": ["../api-page-builder-import-export/src"],
      "@webiny/error/*": ["../error/src/*"],
      "@webiny/error": ["../error/src"],
      "@webiny/handler/*": ["../handler/src/*"],
      "@webiny/handler": ["../handler/src"],
      "@webiny/api-headless-cms/*": ["../api-headless-cms/src/*"],
      "@webiny/api-headless-cms": ["../api-headless-cms/src"],
      "@webiny/api-i18n/*": ["../api-i18n/src/*"],
      "@webiny/api-i18n": ["../api-i18n/src"],
      "@webiny/api-security/*": ["../api-security/src/*"],
      "@webiny/api-security": ["../api-security/src"],
      "@webiny/api-tenancy/*": ["../api-tenancy/src/*"],
      "@webiny/api-tenancy": ["../api-tenancy/src"],
      "@webiny/handler-aws/*": ["../handler-aws/src/*"],
      "@webiny/handler-aws": ["../handler-aws/src"],
      "@webiny/handler-graphql/*": ["../handler-graphql/src/*"],
      "@webiny/handler-graphql": ["../handler-graphql/src"],
      "@webiny/plugins/*": ["../plugins/src/*"],
      "@webiny/plugins": ["../plugins/src"]
    },
    "baseUrl": "."
  }
}

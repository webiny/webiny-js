{
  "extends": "../../tsconfig.build.json",
  "include": ["src"],
  "references": [
    {
      "path": "../error"
    }
  ],
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "declarationDir": "./dist",
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/error/*": ["../error/src/*"],
      "@webiny/error": ["../error/src"]
    },
    "baseUrl": "."
  }
}

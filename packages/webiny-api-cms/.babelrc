{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "node": "8.10"
            }
        }],
        "@babel/preset-flow"
    ],
    "plugins": [
        ["@babel/plugin-proposal-object-rest-spread", {"useBuiltIns": true}],
        ["@babel/plugin-transform-runtime"],
        ["@babel/plugin-syntax-dynamic-import"],
        ["babel-plugin-dynamic-import-node"]
    ]
}
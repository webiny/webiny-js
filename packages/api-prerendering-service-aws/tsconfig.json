{
  "extends": "../../tsconfig.json",
  "include": ["src", "__tests__/**/*.ts"],
  "references": [
    { "path": "../api-prerendering-service" },
    { "path": "../error" },
    { "path": "../api" },
    { "path": "../handler-fastify-aws" },
    { "path": "../handler-client" }
  ],
  "compilerOptions": {
    "rootDirs": ["./src", "./__tests__"],
    "outDir": "./dist",
    "declarationDir": "./dist",
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/api-prerendering-service/*": ["../api-prerendering-service/src/*"],
      "@webiny/api-prerendering-service": ["../api-prerendering-service/src"],
      "@webiny/api/*": ["../api/src/*"],
      "@webiny/api": ["../api/src"],
      "@webiny/error/*": ["../error/src/*"],
      "@webiny/error": ["../error/src"],
      "@webiny/handler-fastify-aws/*": ["../handler-fastify-aws/src/*"],
      "@webiny/handler-fastify-aws": ["../handler-fastify-aws/src"],
      "@webiny/handler-client/*": ["../handler-client/src/*"],
      "@webiny/handler-client": ["../handler-client/src"]
    },
    "baseUrl": "."
  }
}

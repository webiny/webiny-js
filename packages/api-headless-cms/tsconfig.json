{
  "extends": "../../tsconfig",
  "include": ["src", "__tests__/**/*.ts"],
  "references": [
    {
      "path": "../api-i18n"
    },
    {
      "path": "../api-i18n-ddb"
    },
    {
      "path": "../api-i18n-content"
    },
    {
      "path": "../api-security"
    },
    {
      "path": "../api-tenancy"
    },
    {
      "path": "../api-file-manager"
    },
    {
      "path": "../api-upgrade"
    },
    {
      "path": "../error"
    },
    {
      "path": "../handler"
    },
    {
      "path": "../handler-aws"
    },
    {
      "path": "../handler-graphql"
    },
    {
      "path": "../handler-db"
    },
    {
      "path": "../handler-http"
    },
    {
      "path": "../plugins"
    },
    {
      "path": "../validation"
    }
  ],
  "compilerOptions": {
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/api-i18n/*": ["../api-i18n/src/*"],
      "@webiny/api-i18n": ["../api-i18n/src"],
      "@webiny/api-i18n-ddb/*": ["../api-i18n-ddb/src/*"],
      "@webiny/api-i18n-ddb": ["../api-i18n-ddb/src"],
      "@webiny/api-i18n-content/*": ["../api-i18n-content/src/*"],
      "@webiny/api-i18n-content": ["../api-i18n-content/src"],
      "@webiny/api-security/*": ["../api-security/src/*"],
      "@webiny/api-security": ["../api-security/src"],
      "@webiny/api-tenancy/*": ["../api-tenancy/src/*"],
      "@webiny/api-tenancy": ["../api-tenancy/src"],
      "@webiny/api-file-manager/*": ["../api-file-manager/src/*"],
      "@webiny/api-file-manager": ["../api-file-manager/src"],
      "@webiny/api-upgrade/*": ["../api-upgrade/src/*"],
      "@webiny/api-upgrade": ["../api-upgrade/src"],
      "@webiny/error/*": ["../error/src/*"],
      "@webiny/error": ["../error/src"],
      "@webiny/handler/*": ["../handler/src/*"],
      "@webiny/handler": ["../handler/src"],
      "@webiny/handler-aws/*": ["../handler-aws/src/*"],
      "@webiny/handler-aws": ["../handler-aws/src"],
      "@webiny/handler-db/*": ["../handler-db/src/*"],
      "@webiny/handler-db": ["../handler-db/src"],
      "@webiny/handler-graphql/*": ["../handler-graphql/src/*"],
      "@webiny/handler-graphql": ["../handler-graphql/src"],
      "@webiny/handler-http/*": ["../handler-http/src/*"],
      "@webiny/handler-http": ["../handler-http/src"],
      "@webiny/plugins/*": ["../plugins/src/*"],
      "@webiny/plugins": ["../plugins/src"],
      "@webiny/validation/*": ["../validation/src/*"],
      "@webiny/validation": ["../validation/src"]
    },
    "baseUrl": "."
  }
}

{
  "extends": "../../tsconfig.json",
  "include": ["src", "__tests__/**/*.ts"],
  "references": [
    { "path": "../app" },
    { "path": "../app-admin" },
    { "path": "../app-admin-rmwc" },
    { "path": "../app-apw" },
    { "path": "../app-file-manager" },
    { "path": "../app-file-manager-s3" },
    { "path": "../app-form-builder" },
    { "path": "../app-graphql-playground" },
    { "path": "../app-headless-cms" },
    { "path": "../app-i18n" },
    { "path": "../app-i18n-content" },
    { "path": "../app-page-builder" },
    { "path": "../app-security" },
    { "path": "../app-security-access-management" },
    { "path": "../app-tenancy" },
    { "path": "../app-tenant-manager" },
    { "path": "../plugins" }
  ],
  "compilerOptions": {
    "rootDirs": ["./src", "./__tests__"],
    "outDir": "./dist",
    "declarationDir": "./dist",
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/app/*": ["../app/src/*"],
      "@webiny/app": ["../app/src"],
      "@webiny/app-admin/*": ["../app-admin/src/*"],
      "@webiny/app-admin": ["../app-admin/src"],
      "@webiny/app-admin-rmwc/*": ["../app-admin-rmwc/src/*"],
      "@webiny/app-admin-rmwc": ["../app-admin-rmwc/src"],
      "@webiny/app-apw/*": ["../app-apw/src/*"],
      "@webiny/app-apw": ["../app-apw/src"],
      "@webiny/app-file-manager/*": ["../app-file-manager/src/*"],
      "@webiny/app-file-manager": ["../app-file-manager/src"],
      "@webiny/app-file-manager-s3/*": ["../app-file-manager-s3/src/*"],
      "@webiny/app-file-manager-s3": ["../app-file-manager-s3/src"],
      "@webiny/app-form-builder/*": ["../app-form-builder/src/*"],
      "@webiny/app-form-builder": ["../app-form-builder/src"],
      "@webiny/app-graphql-playground/*": ["../app-graphql-playground/src/*"],
      "@webiny/app-graphql-playground": ["../app-graphql-playground/src"],
      "@webiny/app-headless-cms/*": ["../app-headless-cms/src/*"],
      "@webiny/app-headless-cms": ["../app-headless-cms/src"],
      "@webiny/app-i18n/*": ["../app-i18n/src/*"],
      "@webiny/app-i18n": ["../app-i18n/src"],
      "@webiny/app-i18n-content/*": ["../app-i18n-content/src/*"],
      "@webiny/app-i18n-content": ["../app-i18n-content/src"],
      "@webiny/app-page-builder/*": ["../app-page-builder/src/*"],
      "@webiny/app-page-builder": ["../app-page-builder/src"],
      "@webiny/app-security/*": ["../app-security/src/*"],
      "@webiny/app-security": ["../app-security/src"],
      "@webiny/app-security-access-management/*": ["../app-security-access-management/src/*"],
      "@webiny/app-security-access-management": ["../app-security-access-management/src"],
      "@webiny/app-tenancy/*": ["../app-tenancy/src/*"],
      "@webiny/app-tenancy": ["../app-tenancy/src"],
      "@webiny/app-tenant-manager/*": ["../app-tenant-manager/src/*"],
      "@webiny/app-tenant-manager": ["../app-tenant-manager/src"],
      "@webiny/plugins/*": ["../plugins/src/*"],
      "@webiny/plugins": ["../plugins/src"]
    },
    "baseUrl": "."
  }
}

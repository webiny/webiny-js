{
  "extends": "../../tsconfig",
  "include": ["./src", "./__tests__"],
  "references": [
    {
      "path": "../api-i18n-content"
    },
    {
      "path": "../api-plugin-elastic-search-client"
    },
    {
      "path": "../api-security"
    },
    {
      "path": "../api-file-manager"
    },
    {
      "path": "../api-upgrade"
    },
    {
      "path": "../api-security-tenancy"
    },
    {
      "path": "../error"
    },
    {
      "path": "../handler"
    },
    {
      "path": "../handler-client"
    },
    {
      "path": "../handler-graphql"
    },
    {
      "path": "../validation"
    },
    {
      "path": "../api-dynamodb-to-elasticsearch"
    }
  ],
  "compilerOptions": {
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/api-i18n-content/*": ["../api-i18n-content/src/*"],
      "@webiny/api-i18n-content": ["../api-i18n-content/src"],
      "@webiny/api-plugin-elastic-search-client/*": ["../api-plugin-elastic-search-client/src/*"],
      "@webiny/api-plugin-elastic-search-client": ["../api-plugin-elastic-search-client/src"],
      "@webiny/api-security/*": ["../api-security/src/*"],
      "@webiny/api-security": ["../api-security/src"],
      "@webiny/api-upgrade/*": ["../api-upgrade/src/*"],
      "@webiny/api-upgrade": ["../api-upgrade/src"],
      "@webiny/api-security-tenancy/*": ["../api-security-tenancy/src/*"],
      "@webiny/api-security-tenancy": ["../api-security-tenancy/src"],
      "@webiny/error/*": ["../error/src/*"],
      "@webiny/error": ["../error/src"],
      "@webiny/handler/*": ["../handler/src/*"],
      "@webiny/handler": ["../handler/src"],
      "@webiny/handler-client/*": ["../handler-client/src/*"],
      "@webiny/handler-client": ["../handler-client/src"],
      "@webiny/handler-graphql/*": ["../handler-graphql/src/*"],
      "@webiny/handler-graphql": ["../handler-graphql/src"],
      "@webiny/validation/*": ["../validation/src/*"],
      "@webiny/validation": ["../validation/src"],
      "@webiny/api-dynamodb-to-elasticsearch/*": ["../api-dynamodb-to-elasticsearch/src/*"],
      "@webiny/api-dynamodb-to-elasticsearch": ["../api-dynamodb-to-elasticsearch/src"],
      "@webiny/api-file-manager/*": ["../api-file-manager/src/*"],
      "@webiny/api-file-manager": ["../api-file-manager/src"]
    },
    "baseUrl": "."
  }
}

{
  "extends": "../../tsconfig",
  "references": [
    {
      "path": "../api-security"
    },
    {
      "path": "../handler"
    },
    {
      "path": "../handler-http"
    }
  ],
  "compilerOptions": {
    "paths": {
      "@webiny/api-security/*": ["../api-security/src/*"],
      "@webiny/api-security": ["../api-security/src"],
      "@webiny/handler/*": ["../handler/src/*"],
      "@webiny/handler": ["../handler/src"],
      "@webiny/handler-http/*": ["../handler-http/src/*"],
      "@webiny/handler-http": ["../handler-http/src"]
    },
    "baseUrl": "."
  }
}

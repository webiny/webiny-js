{
  "extends": "../../tsconfig",
  "include": ["src"],
  "references": [
    {
      "path": "../app"
    },
    {
      "path": "../app-security"
    },
    {
      "path": "../form"
    },
    {
      "path": "../plugins"
    },
    {
      "path": "../react-router"
    },
    {
      "path": "../ui"
    },
    {
      "path": "../ui-composer"
    },
    {
      "path": "../validation"
    }
  ],
  "compilerOptions": {
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/app/*": ["../app/src/*"],
      "@webiny/app": ["../app/src"],
      "@webiny/app-security/*": ["../app-security/src/*"],
      "@webiny/app-security": ["../app-security/src"],
      "@webiny/form/*": ["../form/src/*"],
      "@webiny/form": ["../form/src"],
      "@webiny/plugins/*": ["../plugins/src/*"],
      "@webiny/plugins": ["../plugins/src"],
      "@webiny/react-router/*": ["../react-router/src/*"],
      "@webiny/react-router": ["../react-router/src"],
      "@webiny/ui/*": ["../ui/src/*"],
      "@webiny/ui": ["../ui/src"],
      "@webiny/ui-composer/*": ["../ui-composer/src/*"],
      "@webiny/ui-composer": ["../ui-composer/src"],
      "@webiny/validation/*": ["../validation/src/*"],
      "@webiny/validation": ["../validation/src"]
    },
    "baseUrl": "."
  }
}

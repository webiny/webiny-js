{
  "extends": "../../tsconfig.json",
  "references": [
    {
      "path": "../app"
    },
    {
      "path": "../app-security"
    },
    {
      "path": "../plugins"
    }
  ],
  "compilerOptions": {
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/app/*": ["../app/src/*"],
      "@webiny/app": ["../app/src"],
      "@webiny/app-security/*": ["../app-security/src/*"],
      "@webiny/app-security": ["../app-security/src"],
      "@webiny/plugins/*": ["../plugins/src/*"],
      "@webiny/plugins": ["../plugins/src"]
    },
    "baseUrl": "."
  }
}

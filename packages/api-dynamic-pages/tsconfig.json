{
  "extends": "../../tsconfig",
  "include": ["./src", "./__tests__"],
  "references": [
    {
      "path": "../api-page-builder"
    },
    {
      "path": "../handler-graphql"
    },
    {
      "path": "../plugins"
    }
  ],
  "compilerOptions": {
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/api-page-builder/*": ["../api-page-builder/src/*"],
      "@webiny/api-page-builder": ["../api-page-builder/src"],
      "@webiny/handler-graphql/*": ["../handler-graphql/src/*"],
      "@webiny/handler-graphql": ["../handler-graphql/src"],
      "@webiny/plugins/*": ["../plugins/src/*"],
      "@webiny/plugins": ["../plugins/src"]
    },
    "baseUrl": "."
  }
}

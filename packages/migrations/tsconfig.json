{
  "extends": "../../tsconfig.json",
  "include": ["src", "__tests__"],
  "references": [
    { "path": "../api-elasticsearch" },
    { "path": "../data-migration" },
    { "path": "../db-dynamodb" },
    { "path": "../error" },
    { "path": "../ioc" },
    { "path": "../logger" },
    { "path": "../utils" },
    { "path": "../api-headless-cms" },
    { "path": "../api-headless-cms-ddb-es" },
    { "path": "../handler-aws" },
    { "path": "../plugins" }
  ],
  "compilerOptions": {
    "rootDirs": ["./src", "./__tests__"],
    "outDir": "./dist",
    "declarationDir": "./dist",
    "paths": {
      "~/*": ["./src/*"],
      "~tests/*": ["./__tests__/*"],
      "@webiny/api-elasticsearch/*": ["../api-elasticsearch/src/*"],
      "@webiny/api-elasticsearch": ["../api-elasticsearch/src"],
      "@webiny/data-migration/*": ["../data-migration/src/*"],
      "@webiny/data-migration": ["../data-migration/src"],
      "@webiny/db-dynamodb/*": ["../db-dynamodb/src/*"],
      "@webiny/db-dynamodb": ["../db-dynamodb/src"],
      "@webiny/error/*": ["../error/src/*"],
      "@webiny/error": ["../error/src"],
      "@webiny/ioc/*": ["../ioc/src/*"],
      "@webiny/ioc": ["../ioc/src"],
      "@webiny/logger/*": ["../logger/src/*"],
      "@webiny/logger": ["../logger/src"],
      "@webiny/utils/*": ["../utils/src/*"],
      "@webiny/utils": ["../utils/src"],
      "@webiny/api-headless-cms/*": ["../api-headless-cms/src/*"],
      "@webiny/api-headless-cms": ["../api-headless-cms/src"],
      "@webiny/api-headless-cms-ddb-es/*": ["../api-headless-cms-ddb-es/src/*"],
      "@webiny/api-headless-cms-ddb-es": ["../api-headless-cms-ddb-es/src"],
      "@webiny/handler-aws/*": ["../handler-aws/src/*"],
      "@webiny/handler-aws": ["../handler-aws/src"],
      "@webiny/plugins/*": ["../plugins/src/*"],
      "@webiny/plugins": ["../plugins/src"]
    },
    "baseUrl": "."
  }
}
